<% content_for :second_menu do %>
  <%= render partial: 'item_listing_item', collection: @items, as: 'item', layout: 'item_listing' unless @items.blank? %>
<% end %>


<%= no_cms_admin_form_for @item do |f| %>
  <%= render partial: 'no_cms/admin/shared/toolbar', locals: { f: f } %>
  <div class="content">
    <%= render 'no_cms/admin/shared/log_messages' %>
    <%= render 'content_fields', f: f %>
  </div>
<% end %>
